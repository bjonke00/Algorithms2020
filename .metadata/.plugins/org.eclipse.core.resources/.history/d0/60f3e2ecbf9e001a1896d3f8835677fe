package com.algorithmpractice;

public class MultiplyNumbersAsStrings {

	public String multiplyNumbersAsStrings(String a, String b){
		StringBuilder sb = new StringBuilder();
		String solution = new String(sb);
		int carry = 0;
		char[] charArrayA = a.toCharArray();
		char[] charArrayB = b.toCharArray();
		for(int i=charArrayA.length-1; i>=0; i--) {
			for(int j=charArrayB.length-1; j>=0; j--) {
				int product = charArrayA[i]*charArrayB[j];
			}
		}
		
		return solution;
	}
	
	
	public static void main(String[] args) {
		
	}
}
